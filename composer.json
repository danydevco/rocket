{
    "name": "danydev/rocket",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Danydev\\Rocket\\": [
                "src/",
                "database/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danydev\\Rocket\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Daniel Jose Ruiz Gutierrez",
            "email": "hello@danydev.co",
            "homepage": "https://danydev.co",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-permission": "^6.0",
        "laravel/helpers": "^1.7",
        "nesbot/carbon": "^2.72"
    },
    "require-dev": {
        "orchestra/testbench": "^9.2"
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=rocket-config"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danydev\\Rocket\\Providers\\RocketServiceProvider",
                "Danydev\\Rocket\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "Rocket": "Danydev\\Rocket\\Facades\\Rocket"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
